<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<!-- 
			Add your own converters using entries of the following form: 
			<bean class="converters.Class1ToClass2" /> 
		-->
		
		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.AdministratorToStringConverter" />
		<bean class="converters.BannerToStringConverter" />
		<bean class="converters.CategoryToStringConverter" />
		<bean class="converters.CreditCardToStringConverter" />
		<bean class="converters.CriticToStringConverter" />
		<bean class="converters.DeveloperToStringConverter" />
		<bean class="converters.DiscountToStringConverter" />
		<bean class="converters.GameToStringConverter" />
		<bean class="converters.MessageEmailToStringConverter" />
		<bean class="converters.ReviewToStringConverter" />
		<bean class="converters.CustomerToStringConverter" />
		<bean class="converters.ReceiptToStringConverter" />
		<bean class="converters.OrderedGamesToStringConverter" />
		<bean class="converters.CommentToStringConverter" />
		<bean class="converters.SenseToStringConverter" />
		<bean class="converters.ShoppingCartToStringConverter" />
		
		<bean class="converters.StringToActorConverter" /> 
		<bean class="converters.StringToAdministratorConverter" /> 
		<bean class="converters.StringToBannerConverter" /> 
		<bean class="converters.StringToCategoryConverter" /> 
		<bean class="converters.StringToCreditCardConverter" /> 
		<bean class="converters.StringToCriticConverter" /> 
		<bean class="converters.StringToDeveloperConverter" /> 
		<bean class="converters.StringToDiscountConverter" /> 
		<bean class="converters.StringToGameConverter" /> 
		<bean class="converters.StringToMessageEmailConverter" /> 
		<bean class="converters.StringToReviewConverter" /> 
		<bean class="converters.StringToCustomerConverter" /> 
		<bean class="converters.StringToReceiptConverter" /> 
		<bean class="converters.StringToOrderedGamesConverter" /> 
		<bean class="converters.StringToCommentConverter" /> 
		<bean class="converters.StringToSenseConverter" /> 
		<bean class="converters.StringToShoppingCartConverter" /> 
		
	</util:list>
	
</beans>